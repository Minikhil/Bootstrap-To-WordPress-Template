<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <security>
            <authorization>
                <remove users="*" roles="" verbs="" />
                <add accessType="Allow" users="*" />
            </authorization>
            <requestFiltering>
                <alwaysAllowedUrls>
                    <add url="/subboard.buffalo.edu" />
                </alwaysAllowedUrls>
            </requestFiltering>
        </security>
        <defaultDocument enabled="true">
            <files>
                <clear />
                <add value="index.html" />
            </files>
        </defaultDocument>
        <handlers accessPolicy="Read, Script">
            <remove name="Perl-cgi" />
            <remove name="Perl-pl" />
            <add name="Perl-pl" path="*.pl" verb="*" modules="CgiModule" scriptProcessor="C:\Perl\bin\perl.exe &quot;%s&quot; %s" resourceType="File" requireAccess="Script" />
        </handlers>
        <httpRedirect enabled="false" destination="https://subboard.com:8081" exactDestination="true" childOnly="false" httpResponseStatus="Permanent" />
        <rewrite>
            <rewriteMaps>
                <rewriteMap name="https://subboard.com">
                    <add key="http://subboard.com" value="https://subboard.com" />
                    <add key="http://www.subboard.com" value="https://subboard.com" />
                    <add key="subboard.com" value="https://subboard.com" />
                </rewriteMap>
                <rewriteMap name="offcampushousing.buffalo.edu">
                </rewriteMap>
                <rewriteMap name="och">
                    <add key="offcampushousing.buffalo.edu/" value="subboard.com/och" />
                </rewriteMap>
            </rewriteMaps>
            <rules>
                <rule name="CanonicalHostNameRule1" enabled="false" stopProcessing="true">
                    <match url="(.*)" />
                    <conditions>
                        <add input="{HTTP_HOST}" pattern="^subboard\.com$" negate="true" />
                    </conditions>
                    <action type="Redirect" url="https://subboard.com:8081/index.html" />
                </rule>
                <rule name="Redirect to HTTPS" enabled="true" stopProcessing="true">
                    <match url="(.*)" />
                    <conditions>
                        <add input="{HTTPS}" pattern="^OFF$" />
                    </conditions>
                    <action type="Redirect" url="https://subboard.com:8081/index.html" />
                </rule>
            </rules>























        </rewrite>














      











    </system.webServer>
</configuration>
